{
    "manifest_version": 3,
    "name": "NovleToEpub",
    "version": "1.0",
    "description": "This extension helps you scrape novel content from websites, currently this only work in Novelhall.com",
    "permissions": [
      "activeTab",
      "tabs",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon/n-16.png",
        "48": "icon/n-32.png",
        "128": "icon/n-512.png"
    }
  }
}
  